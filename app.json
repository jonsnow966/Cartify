{
  "expo": {
    "name": "Cartify",
    "slug": "Cartify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cartify-foreground.png",
    "scheme": "cartify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cartify-foreground.png",
        "backgroundColor": "#1e072e"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jonsnow966.Cartify"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/cartify-foreground.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cartify-foreground.png",
          "resizeMode": "contain",
          "backgroundColor": "#1e072e"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "11b7d4d1-b6e3-4643-908a-05f2d7f9b374"
      }
    }
  }
}